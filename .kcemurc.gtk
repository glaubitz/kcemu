# pixmap_path "<dir 1>:<dir 2>:<dir 3>:..."
#
# include "rc-file"
#
# style <name> [= <name>]
# {
#   <option>
# }
#
# widget <widget_set> style <style_name>
# widget_class <widget_class_set> style <style_name>

style "led"
{
	fg[NORMAL] = { 0.1, 0.3, 1.0 }
}
style "green_led"
{
	fg[NORMAL] = { 0.0, 0.8, 0.0 }
}

widget_class "*GtkLedLine" style "led"

widget "ModuleWindow.*GtkLedLine" style "green_led"
